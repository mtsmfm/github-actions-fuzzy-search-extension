{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "name": "GitHub Actions Fuzzy Search",
  "description": "Fuzzy search for GitHub Actions",
  "version": "0.1.0",
  "permissions": ["scripting", "storage"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["src/content.tsx"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 3
}
